<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp">
        <shape android:shape="rectangle">
            <corners android:radius="50dp" />
            <solid android:color="#fff" />
        </shape>
    </item>
    <item android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp">
        <shape android:shape="oval">
            <stroke android:width="2dp" android:color="#fff" android:dashWidth="5dp" android:dashGap="3dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:width="2dp" android:color="#99ffffff" android:dashWidth="5dp" android:dashGap="3dp" />
        </shape>
    </item>
</layer-list>
